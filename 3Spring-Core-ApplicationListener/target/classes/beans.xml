<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN"
        "http://www.springframework.org/dtd/spring-beans-2.0.dtd">

<beans>
	<bean id="Employee" class="com.spring.core.EmployeeBean" init-method="init" destroy-method="destroy">
	<property name="name" value="sathiya"></property>
	</bean>
	<bean id="contextStart" class="com.spring.core.ContextStart"></bean>
	<bean id="contextStop" class="com.spring.core.ContextStop"></bean>
	<bean id="contextClose" class="com.spring.core.ContextClose"></bean>
	<bean id="contextRefresh" class="com.spring.core.ContextRefresh"></bean>
</beans>